<h1>Listing agenda_record_entries</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Start time</th>
    <th>End time</th>
  </tr>

<% @agenda_record_entries.each do |agenda_record_entry| %>
  <tr>
    <td><%=h agenda_record_entry.title %></td>
    <td><%=h agenda_record_entry.start_time %></td>
    <td><%=h agenda_record_entry.end_time %></td>
    <td><%= link_to 'Show', agenda_record_entry %></td>
    <td><%= link_to 'Edit', edit_agenda_record_entry_path(agenda_record_entry) %></td>
    <td><%= link_to 'Destroy', agenda_record_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New agenda_record_entry', new_space_event_agenda_agenda_record_entry_path %>