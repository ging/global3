<% if current_user.groups.any? %>
<div id="my_spaces">
    <div id="title_space" class="content_size">
        <%=t('space.my_spaces') %>
    </div>
    <div id="spaces_list" class="content_size">
		<% current_user.groups.each do |group| %>
                <%=link_to(image_tag(group.logo, :class => 'btn_config', :alt => group.name, :title => group.name), group) %>
		<% end %>
    </div>
</div>
<% else %>
<div class="space_center">
</div>
<% end %>



