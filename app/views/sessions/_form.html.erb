<%= form_for @session ,:url=> event_agenda_sessions_path(@event) ,:remote => true do |f| %>

  <div class="form_row">
    <%= label_tag :name %>
  </div>
  
  <div class="form_row">
    <%= f.text_field :name %>
  </div>

 <div class="form_row">
    <%= label_tag :start_at%>
  </div>
  
  <div class="form_row">
    <%= f.datetime_select :start_at %>
  </div>

  <div class="form_row">
    <%= label_tag :end_at%>
  </div>
  
  <div class="form_row">
    <%= f.datetime_select :end_at %>
  </div>

  <div class="form_row">
    <%= f.submit "Edit event", :class => "button" %>
  </div>
<% end %>